rules_version = '2';

service cloud.firestore {
  match /databases/{database}/documents {
    match /Events/{eventId} {
      allow read, write: if request.time < timestamp.date(2024, 12, 25);
    }
    match /Guests/{guestId} {
      allow read, write: if request.time < timestamp.date(2024, 12, 25);
    }
  }
}
